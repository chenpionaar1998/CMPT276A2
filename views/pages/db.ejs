<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Database Results</h2>

<ul>
    <% results.forEach(function(r) { %>
        <li><%= r.studentid %> - <%= r.fname %> - 
          <%= r.lname %> - <%= r.dob %> - <%= r.gender %> - 
          <%= r.weight %> - <%= r.height %> - <%= r.shoe_size %> - 
          <%= r.hair_color %> - <%= r.gpa %> </li>
    <% }); %>
</ul>

</div>

</body>
</html>
